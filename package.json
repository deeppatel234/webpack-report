{
  "name": "webpack-report",
  "version": "2.0.0",
  "description": "Webpack Build Analysis Plugin",
  "main": "src/index.js",
  "files": [
    "dist",
    "src/index.js",
    "src/server",
    "src/utils",
    "config/paths.js"
  ],
  "scripts": {
    "start": "concurrently \"webpack-dev-server --progress --config config/webpack.dev.js\" \"cd example && npm run build\"",
    "start:dev": "webpack-dev-server --progress --config config/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:dev": "webpack --config config/webpack.dev.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeppatel234/webpack-report.git"
  },
