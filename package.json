  "bugs": {
    "url": "https://github.com/deeppatel234/webpack-report/issues"
  },
  "homepage": "https://github.com/deeppatel234/webpack-report#readme",
  "dependencies": {
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^4.2.9",
